.brand .sec01{padding-bottom:75px;}
.brand .sec01 .center-content-box,
.brand .sec02 .center-content-box{justify-content: space-between;}

.brand .section .txt-box .sub-sec-sub-tit{padding-bottom:20px;}

.brand .sec02{padding-bottom:175px;}
.brand .sec02 .txt-box{text-align:left;}
