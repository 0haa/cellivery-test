.sub_banner .sub_nav li.active .svgImg{opacity:0.3; transform: translate(-50%,-50%) scale(2.5);}

.sub_container{width:77%; margin:0 auto; padding:115px 0 125px;}
.sub_top_box{display: flex; flex-direction: row; justify-content: space-between; padding-bottom:30px;}
