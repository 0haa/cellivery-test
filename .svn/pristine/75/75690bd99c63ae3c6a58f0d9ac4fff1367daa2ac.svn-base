.sub_search_box > *{ height:100%; border:1px solid #f3f3f3; padding:0 20px; display:flex; align-items: center; justify-content: space-between;}
.sub_search_box .select_box{width:190px; position:relative; cursor:pointer; border-radius:5px;}
.sub_search_box .select_box.active img{transform: rotate(180deg);}
.sub_search_box .select_box p{color:#262f40; font-size:18px; font-weight:bold;}
/* .sub_search_box .select_box img{display:block;} */
.sub_search_box .select_box ul{display:none; position:absolute; top:100%; left:-1px; width:calc(100% + 1px); border: 1px solid #f3f3f3; border-bottom: 0; z-index:2;}
