.youtube_ul li .li_tit,
.sns_big .li_tit{padding-top:1.052vw; padding-bottom:10px; font-size:14px; color:#989898; transition:all 0.3s;}
.youtube_ul li .sm_txt,
.sns_big .sm_txt{font-size:20px; color:#000; font-weight:500; white-space: nowrap;  text-overflow: ellipsis; overflow:hidden;; transition:all 0.3s;}
.sns .sub_search_box .select_box{width:235px;}


.insta_ul{display:flex;}
.insta_ul li{width:calc((99.999% - (25px*5)) / 6); margin-right:25px; position:relative; border-radius:10px; overflow:hidden; cursor:pointer;}
.insta_ul li:last-child{margin-right:0;}
.insta_ul li .txt_box{position:absolute; top:0; left:0; width:100%; height:100%;  display:flex; align-items:center; justify-content:center; flex-direction: column; text-align: center;background:#9fbee4; color:#fff; opacity:0; transition:all 0.3s;}
.insta_ul li .txt_box .tit_txt{ color:inherit; font-size:20px;}
.insta_ul li .txt_box .con_txt{color:inherit; font-size:16px;  width:100%;  height: 45px; line-height:1.38; padding:0; margin:15px 0;    word-break: keep-all; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;  overflow: hidden;}
.insta_ul li:hover .txt_box{opacity:1;}
.insta_ul li .browse-more{justify-content: center;}
.insta_ul li .svgImg{margin-left:0;}

.insta_ul li .browse-more .link-wrap{width: 100%; justify-content: center;}
.insta_ul li .browse-more .svgImg{width:80px; height:80px;}
.insta_ul li .browse-more:hover .svgImg{opacity:0}
.insta_ul li .txt_box:hover .browse-more .svgImg{opacity:0.3;}
.insta_ul li .browse-more:hover .link-wrap img{margin:0;}
.insta_ul li .txt_box:hover .browse-more .link-wrap img{margin:0;}
.insta_ul li .txt_box:hover .browse-more g {transform: scale(1.1);}


/* brand */
