/* career */
.career .sub_content{padding:130px 0 ;position:relative;}
.career .sub_content > img{position:absolute; top:0; left:0;}
.career .sub_banner {background: url(/img/sub/career/career_banner_bg.jpg) no-repeat center / cover;}
.career .sub-sec-tit-box{text-align:center;}
.career .sub-sec-sub-tit{font-size: 49px; color:#000;font-weight: bold; padding-bottom:60px;}
.career .center-content-box{max-width:1130px; width:100%; margin:0 auto;padding:0;}

.career .sec01{padding-bottom:100px; position:relative;}
.career .sec01 > img{position:absolute; bottom:0; right:0;}
.career .sec01 .center-content-box{flex-direction:column; justify-content:center; align-items: center;}
.career .sec01 .center-content-box ul{max-width:845px; width:100%; margin:30px auto 0; display:flex;}
.career .sec01 .center-content-box ul li{width:33.333%; text-align:center;}
.career .sec01 .center-content-box ul li:nth-child(1){padding-right:60px}
.career .sec01 .center-content-box ul li:nth-child(3){padding-left:70px}
.career .sec01 .center-content-box ul li .tit{font-size: 22.5px; font-weight: bold; color: #1f232d;padding-bottom:20px;}
.career .sec01 .center-content-box ul li .con{font-size: 16px; font-weight: normal; line-height: 1.75;  letter-spacing: -0.8px;color: rgba(51, 51, 51, 0.8);}

.career .sec02{padding:140px 0 155px;background: url(/img/sub/career/career_mid_banner.jpg) no-repeat center / cover;}
.career .sec02 .center-content-box{max-width:910px; width:100%; align-items: center;}
.career .sec02 .center-content-box li{position:relative;    margin-right: 15px;}
.career .sec02 .center-content-box li:last-child{margin:0;}
.career .sec02 .center-content-box li img{display:block; margin:0 20px;}
.career .sec02 .center-content-box li .num{font-size: 210.5px;font-weight: bold; color: rgba(0, 0, 0, 0.05);}
.career .sec02 .center-content-box li .con{font-size: 24px;font-weight: bold;  letter-spacing: -1.2px;color: #000; position:absolute; top:50%; left:50%; transform:translate(-50%,-50%);}
.career .sec02 .center-content-box li .con:before {content: ''; display: block; width: 102%; height: 10px; background: #9fbee4; opacity: 0.7;  position: absolute; bottom: -2px;  z-index: -1; left: 50%;  transform: translateX(-50%);}


.career .sec03 {padding-top:100px;}
.career .sec03 .center-content-box{flex-wrap: wrap;}
.career .sec03 .center-content-box li{margin-right:17px; width:calc((99.999% - 34px)/3); height:270px;border-radius: 10px;  background-color: #f4f5f9;padding:45px 40px; position:relative;}
.career .sec03 .center-content-box li:nth-child(3n){margin-right:0;}
.career .sec03 .center-content-box li:nth-last-child(-n+3){margin-top:15px;}
.career .sec03 .center-content-box li .num{font-size: 19.5px; font-weight: bold; color: #a9c4de; padding-bottom:20px;}
.career .sec03 .center-content-box li .con{font-size: 26.5px; font-weight: bold; line-height: 1.4;color: #1f232d;}
.career .sec03 .center-content-box li img{position:absolute; bottom:40px; right:40px;; display:block;}






