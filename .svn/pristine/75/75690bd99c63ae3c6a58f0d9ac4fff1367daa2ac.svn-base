.brand-sec04-box li{width:220px; height:220px; border-radius:50%; display:flex; align-items:center; justify-content:center; text-align: center; background: #c5d8ef; margin-top: -45px;  mix-blend-mode: multiply;}
.brand-sec04-box li:first-child{margin-top:0;}
.brand-sec04-box li p{font-size: 24px;line-height: 1.42;color: #1f232d;}
