.news_table{border-top:1px solid #000;  width: 100%; text-align: center; display:table;}
.news_table > div{display:table-row}
.news_table > div > div{display:table-cell;height:95px; color:#202020; font-size:18px; border-bottom:1px solid #e5e5e5; vertical-align: middle;}
.news_table th, .news_table td{}
